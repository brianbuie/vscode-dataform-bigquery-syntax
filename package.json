{
  "name": "dataform-bigquery-syntax",
  "displayName": "dataform-bigquery-syntax",
  "description": "syntax highlighting for sqlx files and bigquery flavored sql",
  "version": "0.0.1",
  "engines": {
    "vscode": "^1.78.0"
  },
  "categories": [
    "Programming Languages"
  ],
  "contributes": {
    "languages": [{
      "id": "sqlx",
      "aliases": ["bq-sqlx", "sqlx"],
      "extensions": [".sqlx"],
      "configuration": "./language-configuration.json"
    }],
    "grammars": [{
      "language": "sqlx",
      "scopeName": "source.sqlx",
      "path": "./syntaxes/sqlx.tmLanguage.json"
    }]
  }
}
